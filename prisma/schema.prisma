generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  name     String
  stravaId Int
  goals    Goal[]
}

model Goal {
  sportType  String
  kilometers String
  userId     String
  goalDate   String
  startDate  String
  id         Int    @id @default(autoincrement())
  User       User?  @relation(fields: [userId], references: [id])
}
